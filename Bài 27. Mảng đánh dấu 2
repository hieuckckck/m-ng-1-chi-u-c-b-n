#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int dem[1000001];
int main() {
int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
        dem[a[i]]++;
    }
    for(int i=0;i<=1000000;i++){
        if(dem[i]!=0) {printf("%d %d\n",i,dem[i]);}
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

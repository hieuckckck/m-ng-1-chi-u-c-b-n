#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int k=0;
    for(int i=0;i<n;i++){
        if(a[i]%2==0&&i%2==0) {printf("%d ",a[i]);k=1;}
    }
    if(k==0) printf("NONE");
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int x;
    scanf("%d",&x);
    int dem1=0;int dem2=0;
    for(int i=0;i<n;i++){
        if(a[i]<x){
           dem1++;
        }  
        if(a[i]>x) dem2++;
    }
    printf("%d\n%d",dem1,dem2);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
